<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nnazimudeen.outreach" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ebay_icon"
        android:label="Outreach"
        android:theme="@style/MyAppTheme" >
        <activity
            android:name=".LaunchPage"
            android:label="@string/title_activity_launch_page"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Categories"
            android:logo="@drawable/launch">
        </activity>
        <activity
            android:name=".QuestionActivity"
            android:label="@string/title_activity_question"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".ScoreBoardActivity"
            android:label="@string/title_activity_score_board"
            android:parentActivityName=".MainActivity" >

        </activity>
    </application>

    <!--<uses-sdk android:minSdkVersion="15" android:targetSdkVersion="15"/>-->

</manifest>
